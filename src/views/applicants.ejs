<div class="mx-5">
    <table class="table table-striped">
        <thead>
            <tr>
              <th scope="col">#</th>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
              <th scope="col">Contact</th>
              <th scope="col">Resume</th>

            </tr>
          </thead>
          <tbody>
            <%appli.forEach((a)=>{%>
                <tr>
                    <th scope="row">1</th>
                    <td><%=a.name%></td>
                    <td><%=a.email%></td>
                    <td><%=a.contact%></td>
                    <td><button class="btn btn-primary btn-sm">view</button></td>

                  </tr>
            <%})%>

          </tbody>
    </table>
</div>